<p id="notice"><%= notice %></p>

<h1>Listing Notes</h1>

<table>
  <thead>
    <tr>
      <th>Td</th>
      <th>Fecha</th>
      <th>Turno</th>
      <th>Cod emp</th>
      <th>Caja</th>
      <th>Serie</th>
      <th>Numero</th>
      <th>Cod cli</th>
      <th>Ruc</th>
      <th>Placa</th>
      <th>Odometro</th>
      <th>Cod prod</th>
      <th>Cantidad</th>
      <th>Precio</th>
      <th>Importe</th>
      <th>Igv</th>
      <th>Fpago</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td 
        <td  class="small bordered_bottom bordered_right"><%= note.td %></td>
        <td class="small bordered_bottom bordered_right"><%= note.fecha.strftime("%d/%m/%Y") %></td>
        <td class="small bordered_bottom bordered_right"><%= note.turno %></td>
        <td class="small bordered_bottom bordered_right"><%= note.cod_emp %></td>
        <td class="small bordered_bottom bordered_right"><%= note.caja %></td>
        <td class="small bordered_bottom bordered_right"><%= note.serie %></td>
        <td class="small bordered_bottom bordered_right"><%= note.numero %></td>
        <td class="small bordered_bottom bordered_right"><%= note.cod_cli %></td>
        <td class="small bordered_bottom bordered_right"><%= note.ruc %></td>
        <td class="small bordered_bottom bordered_right"><%= note.placa %></td>
        <td class="small bordered_bottom bordered_right"> <%= note.odometro %></td>
        <td class="small bordered_bottom bordered_right"><%= note.cod_prod %></td>
        <td class="small bordered_bottom bordered_right"><%= note.cantidad %></td>
        <td class="small bordered_bottom bordered_right"><%= note.precio %></td>
        <td class="small bordered_bottom bordered_right"><%= note.importe %></td>
        <td class="small bordered_bottom bordered_right"><%= note.igv %></td>
        <td class="small bordered_bottom bordered_right"> <%= note.fpago %></td>
        <td class="small bordered_bottom bordered_right" ><%= link_to 'Show', note %></td>
        <td class="small bordered_bottom bordered_right"><%= link_to 'Edit', edit_note_path(note) %></td>
        <td class="small bordered_bottom bordered_right"><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate @notes  %> 

<br>


<div class="button">
      <%= link_to 'Nueva Nota', new_note_path %>
</div>

<%= form_tag procesar_notes_path do %>
     	    <%= submit_tag  "Procesar ",  :"data-no-turbolink" => true  %>
		<% end %>		


<% if current_user.permission_level=='admin' %>

<h2>Importar Archivo Boletas "*.csv"</h2>
<%= form_tag import_notes_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Importar " %>
<% end %>
<%end %>


